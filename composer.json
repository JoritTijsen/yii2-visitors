{
    "name": "johnsnook/yii2-ip-filter",
    "description": "This extension gets the visitors ip address, logs the access and checks if it's blacklisted or whitelisted.  If it's a blacklist ip address, it redirects them to a blowoff page.",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "access_log", "proxy", "tor", "blacklist", "ip address", "security", "hackers"],
    "homepage": "https://snooky.biz/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "John Michael Snook",
            "email": "jsnook@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-bootstrap4": "*",
        "kassner/log-parser": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "johnsnook\\ipFilter\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
